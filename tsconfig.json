{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "paths": {
      "button-group": [
        "dist/button-group/button-group",
        "dist/button-group",
        "projects/button-group/src/public-api.ts"
      ],
      "core": [
        "dist/core/core",
        "dist/core",
        "projects/core/src/public-api.ts"
      ],
      "data-table": [
        "dist/data-table/data-table",
        "dist/data-table",
        "projects/data-table/src/public-api.ts"
      ],
      "data-tree": [
        "dist/data-tree/data-tree",
        "dist/data-tree",
        "projects/data-tree/src/public-api.ts"
      ],
      "feature": [
        "dist/feature/feature",
        "dist/feature",
        "projects/feature/src/public-api.ts"
      ],
      "micro-front": [
        "dist/micro-front/micro-front",
        "dist/micro-front"
      ],
      "ngrx-wheel": [
        "dist/ngrx-wheel/ngrx-wheel",
        "dist/ngrx-wheel",
        "projects/ngrx-wheel/src/public-api.ts"
      ],
      "page-layout": [
        "dist/page-layout/page-layout",
        "dist/page-layout",
        "projects/page-layout/src/public-api.ts"
      ],
      "search-group": [
        "dist/search-group/search-group",
        "dist/search-group",
        "projects/search-group/src/public-api.ts"
      ]
    },
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
