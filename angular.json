          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngrx-wheel/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngrx-wheel/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
